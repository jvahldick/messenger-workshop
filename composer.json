{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.2@dev",
        "symfony/config": "^4.2@dev",
        "symfony/console": "^4.2@dev",
        "symfony/expression-language": "^4.2@dev",
        "symfony/http-kernel": "^4.2@dev",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.2@dev",
        "symfony/framework-bundle": "^4.2@dev",
        "symfony/messenger": "^4.2@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.2@dev",
        "symfony/security-bundle": "^4.2@dev",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.2@dev",
        "symfony/validator": "^4.2@dev",
        "symfony/web-link": "^4.2@dev",
        "symfony/dependency-injection": "^4.2@dev",
        "symfony/webpack-encore-pack": "*",
        "symfony/yaml": "^4.2@dev",
        "symfony/contracts": "^1.0@dev"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/dotenv": "^4.2@dev",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "^4.2@dev"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
